# Server
PORT=5000
NODE_ENV=development

# Supabase
SUPABASE_URL=your-supabase-url
SUPABASE_SERVICE_KEY=your-supabase-service-key

# JWT
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=7d

# Stripe
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# EasyPost
EASYPOST_API_KEY=your-easypost-api-key

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_DEFAULT=100
RATE_LIMIT_STRICT=20
RATE_LIMIT_RELAXED=300
RATE_LIMIT_WHITELIST=127.0.0.1,::1

# Redis Configuration
REDIS_URL=redis://localhost:6379
CACHE_STORE=memory # Options: memory, redis

# API Key Rate Limiting
API_KEY_RATE_LIMIT_BASIC=100
API_KEY_RATE_LIMIT_PREMIUM=300
API_KEY_RATE_LIMIT_ENTERPRISE=1000
API_KEY_WHITELIST=test_key_1,test_key_2

# Proxy Configuration
TRUSTED_PROXIES=127.0.0.1,::1
